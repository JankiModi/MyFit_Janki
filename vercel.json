{
    "version": 2,
    "routes": [
        {
            "src": "/images/(.*)",
            "dest": "/images/$1"
        },
        {
            "src": "/exercise videos/(.*)",
            "dest": "/exercise videos/$1"
        },
        {
            "src": "/static/(.*)",
            "dest": "/static/$1"
        },
        {
            "src": "/(.*).css",
            "dest": "/$1.css"
        },
        {
            "src": "/favicon.ico",
            "dest": "/favicon.ico"
        },
        {
            "handle": "filesystem"
        },
        {
            "src": "/(.*)",
            "dest": "/index.html"
        }
    ]
}